pipeline {
    agent any

    stages {
        stage("git"){
            steps {
                sh "git https://github.com/cloudperis/spring-petclinic-main.git"
            }


        }
        stage("Build"){
            steps{
                sh "ls"
            }


        }
        stage("Test"){
            steps {
                echo "Testing...."
            }


        }
        stage("Deploy"){
            steps {
                echo "Deploying...."
            }
        }


    }
}