 pipeline {
agent any

stages{

   stage("source"){
           step {
               sh "git clone https://github.com/cloudperis/spring-petclinic-main.git"
           }
 }


 stage("Build"){
step {
     echo "building application process"
}
 }


  stage("test"){
      step {
           echo "testing application in process"
      }
     
  } 


  stage("deploy"){
step {
    echo "deploying application in process"
}
  } 
}



}